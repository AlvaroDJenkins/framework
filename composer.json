{
    "name": "emberfuse/framework",
    "description": "A simple PHP framework",
    "keywords": ["framework", "emberfuse"],
    "type": "project",
    "homepage": "https://emberfuse.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Thavarshan Thayananthajothy",
            "email": "tjthavarshan@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/container": "^1.0",
        "monolog/monolog": "^2.0",
        "symfony/console": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/var-dumper": "^5.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Emberfuse\\": "src/Emberfuse/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emberfuse\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
